;
; This is the INF file for Windows 2008.
[Version]
signature="$Windows NT$"
Class=SCSIAdapter
ClassGUID={4D36E97B-E325-11CE-BFC1-08002BE10318}
CatalogFile=FTT3s.cat
Provider=%Promise%
DriverVer=02/01/2011, 1.3.0.90

[DestinationDirs] 
FTT3s.Files.x86_12 = 12 

[SourceDisksNames.x86]
0=%Desc_x860%

[SourceDisksFiles.x86]
FTT3s.sys=0,

[ControlFlags]
ExcludeFromSelect=*

[Manufacturer] 
%Promise%=Promise

[Promise]
%SATA_TX2650_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F19&SUBSYS_3F29105A
%SATA_TX4650_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F19&SUBSYS_3F2A105A
%SATA_TX4652_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F19&SUBSYS_3F2B105A
%FastTrak TX2650_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F20&SUBSYS_3F21105A
%FastTrak TX4650_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F20&SUBSYS_3F22105A
%FastTrak PDC42819_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F20&SUBSYS_3716105A
%FastTrak TX4652_Desc%=Napa_Inst,PCI\VEN_105A&DEV_3F20&SUBSYS_3F24105A
%FastTrak T2_Desc%=Napa_Inst,PCI\VEN_105A&DEV_377d&SUBSYS_3772105A
%Promise_Raid_ControllerDesc%=NODRV,SCSI\ProcessorPromise_

[NODRV]
CopyFiles=MoveNODRV

[MoveNODRV]

[NODRV.Services]
;
; this is blank to just allow the install to succeed
;
AddService = , %SPSVCINST_ASSOCSERVICE%   ; null service install

[Napa_Inst.ntx86] 
CopyFiles = FTT3s.Files.x86_12 

[Napa_Inst.ntx86.Services] 
AddService = FTT3s,0x00000002,Napa_Service_Instx86, Napa_EventLog_Inst 

[Napa_Service_Instx86] 
ServiceType = %SERVICE_KERNEL_DRIVER% 
StartType = %SERVICE_BOOT_START% 
ErrorControl = %SERVICE_ERROR_NORMAL% 
ServiceBinary = %12%\FTT3s.sys 
LoadOrderGroup = "SCSI Storport"
AddReg = pnpsafe_pci_addreg


[FTT3s.Files.x86_12] 
FTT3s.sys

[Napa_EventLog_Inst] 
AddReg = Napa_EventLog_Inst.AddReg 
 
[Napa_EventLog_Inst.AddReg] 
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll" 
HKR,,TypesSupported,%REG_DWORD%,7

[pnpsafe_pci_addreg] 
HKR, Parameters,BusType,0x00010001,0x00000008
HKR, Parameters\PnpInterface, 5, 0x00010001, 0x00000001
HKR, Parameters\Device, NumberOfRequests, 0x00010001, 0x00000080 
HKR, Settings\CAM, EnableALPEDisableHotplug, 0x00010001, 0x00000000 

[pnpsafe_isa_addreg] 
HKR, Parameters\PnpInterface, 1, 0x00010001, 0x00000001 

[pnpsafe_eisa_addreg] 
HKR, Parameters\PnpInterface, 2, 0x00010001, 0x00000001 


[Strings] 

;  *******Localizable Strings******* 
Promise = "Promise Technologies Inc." 
Desc_x860= "Promise FastTrak TX4650/SATA4650 Series Controller Driver" 
SATA_TX2650_Desc= "Promise SATA TX2650 Controller"
SATA_TX4650_Desc= "Promise SATA TX4650 Controller"
SATA_TX4652_Desc= "Promise SATA TX4652 Controller"
FastTrak TX2650_Desc= "Promise FastTrak TX2650 Controller"
FastTrak TX4650_Desc= "Promise FastTrak TX4650 Controller"
FastTrak TX4652_Desc= "Promise FastTrak TX4652 Controller"
FastTrak PDC42819_Desc= "Promise FastTrak PDC42819 Controller"
FastTrak T2_Desc= "Promise FastTrak SAS879 Controller"
PROMISE_Raid_ControllerDesc= "Promise Raid Console"

;  *******Non Localizable Strings******* 

SERVICE_BOOT_START = 0x0 
SERVICE_SYSTEM_START = 0x1 
SERVICE_AUTO_START = 0x2 
SERVICE_DEMAND_START = 0x3 
SERVICE_DISABLED = 0x4 

SERVICE_KERNEL_DRIVER = 0x1 
SERVICE_ERROR_IGNORE = 0x0 
SERVICE_ERROR_NORMAL = 0x1 
SERVICE_ERROR_SEVERE = 0x2 
SERVICE_ERROR_CRITICAL = 0x3 

REG_EXPAND_SZ = 0x00020000 
REG_DWORD = 0x00010001 

SPSVCINST_ASSOCSERVICE=0x00000002
