[FLxHCIh.NT]
CopyFiles=FLxHCIh.CopyFiles
FeatureScore=0x80

[FLxHCIh.CopyFiles]
FLxHCIh.sys

[FLxHCIh.NT.Services]
AddService=FLxHCIh,0x00000002,FLxHCIh_Service

[FLxHCIh_Service]
DisplayName    = %FLxHCIh.SVCDESC%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\FLxHCIh.sys
AddReg         = FLxHCIx_Parameters_AddReg
LoadOrderGroup = Base

[FLxHCIx_Parameters_AddReg]

[SourceDisksFiles]
FLxHCIh.sys=1

[FLxHCIh.NT.Wdf]
KmdfService = FLxHCIh, FLxHCIh_wdfsect

[FLxHCIh_wdfsect]
KmdfLibraryVersion = 1.9

