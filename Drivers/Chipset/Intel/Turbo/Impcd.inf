[version]
Signature       = "$Windows NT$"
Class           = System
ClassGUID       = {4D36E97D-E325-11CE-BFC1-08002BE10318}
Provider        = %Mfg%
CatalogFile     = Impcd.cat
DriverVer       = 02/10/2010, 01.01.01.1007

[DestinationDirs]
DefaultDestDir = 12

[Manufacturer]
%Mfg%           = IntelDevice,ntamd64

[IntelDevice]



%Impcd.DeviceDesc%    = Impcd,PCI\VEN_8086&DEV_3B32


[IntelDevice.ntamd64]



%Impcd.DeviceDesc%    = Impcd,PCI\VEN_8086&DEV_3B32


;----------------------------------------------------------------------
; NT install sections:
;----------------------------------------------------------------------

;----------------------------------------------------------------------
; Impcd:
;----------------------------------------------------------------------
[Impcd]
CopyFiles       = Impcd.CopyDriver

[Impcd.CopyDriver]
Impcd.sys,,,2

[Impcd.Services]
AddService      = Impcd,2,ImpcdDriverService

[ImpcdDriverService]
ServiceType     = %SERVICE_KERNEL_DRIVER%
StartType       = %SERVICE_DEMAND_START%
ErrorControl    = %SERVICE_ERROR_NORMAL%
ServiceBinary   = %12%\Impcd.sys
AddReg          = Impcd.AddReg
DelReg          = Impcd.DelReg

[Impcd.HW]
AddReg = Impcd_HWSettings

[Impcd_HWSettings]
HKR, "Interrupt Management",, 0x00000010
HKR, "Interrupt Management\MessageSignaledInterruptProperties",, 0x00000010
HKR, "Interrupt Management\MessageSignaledInterruptProperties", MSISupported, 0x00010001, 0

[Impcd.AddReg]

[Impcd.DelReg]

;----------------------------------------------------------------------
; Source file information
;----------------------------------------------------------------------

[SourceDisksNames]
1 = %DiskId%,,,""

[SourceDisksFiles]
Impcd.sys       	= 1,x32

[SourceDisksFiles.amd64]
Impcd.sys       	= 1,x64

;----------------------------------------------------------------------
; String Constants
;----------------------------------------------------------------------
[Strings]
REG_DWORD               = 0x00010001
SERVICE_DEMAND_START    = 3
SERVICE_KERNEL_DRIVER   = 1
SERVICE_ERROR_NORMAL    = 1
Mfg                     = "Intel"
Impcd.DeviceDesc      = "Intel(R) Turbo Boost Technology Driver"

;
; Localizable Strings
;
DiskId                  = "Intel(R) Turbo Boost Technology Driver"
