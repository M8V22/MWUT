[version]
;Layoutfile parameter is reserved for internal Microsoft use only
;LayoutFile = layout.inf
Signature   = "$Chicago$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %Mellanox%
Compatible  = 1
DriverVer=01/27/2011,2.1.3.7290
CatalogFile=mlx4eth6.cat

[Manufacturer]
%Mellanox%     = Mellanox,NTx86,NTia64,NTamd64

[Mellanox.NTx86]
; DisplayName           Section       DeviceID
; -----------           -------       --------
%mlx4eth6.DeviceDesc%  = mlx4eth6.ndi,    MLX4\ConnectX_Eth   ; Mellanox ConnectX Ethernet Adapter


[Mellanox.NTia64]
; DisplayName           Section       DeviceID
; -----------           -------       --------
%mlx4eth6.DeviceDesc%  = mlx4eth6.ndi,    MLX4\ConnectX_Eth   ; Mellanox ConnectX Ethernet Adapter
; Mellanox 82557/82558 Based PCI Ethernet Adapter (10/100) and Mellanox compatibles

[Mellanox.NTamd64]
; DisplayName           Section       DeviceID
; -----------           -------       --------
%mlx4eth6.DeviceDesc%  = mlx4eth6.ndi,    MLX4\ConnectX_Eth   ; Mellanox ConnectX Ethernet Adapter

;

; Windows NT specific entry
[mlx4eth6.ndi.NT]
Characteristics = 0x81
AddReg          = 82557.reg, mlx4eth6.reg, WinCtxTune_reg
CopyFiles       = mlx4eth6.CopyFiles, Wctxtune.CopyFiles
*IfType         = 6		; IF_TYPE_ETHERNET_CSMACD 
*MediaType      = 0		; NdisMedium802_3
*PhysicalMediaType = 14		; NdisPhysicalMedium802_3

[mlx4eth6.ndi.NT.Services]
AddService = mlx4eth6, 2, mlx4eth6.Service, common.EventLog

