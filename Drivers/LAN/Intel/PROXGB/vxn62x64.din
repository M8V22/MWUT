; vxn60x64.din
[Version]
Signature   = "$Windows NT$"
SetupClass  = BASE
Provider    = %Intel%

[Manufacturer]
%Intel%     = Intel

[ControlFlags]

[Intel]

;-----------------------------------------------------------------------------
; DestinationDirs
;
[DestinationDirs]
DefaultDestDir          = 11
vxn.DelFiles          = 12
CoInstaller.DelFiles    = 11
UnInstall.DelFiles      = 11

;----------------------------------------------------------------------------
; Uninstall
[8259x.Uninstall]
DelFiles        = vxn.DelFiles, CoInstaller.DelFiles, UnInstall.DelFiles
DelReg          = 8259x.DelReg

[vxn.DelFiles]
vxn62x64.sys,,,1

[CoInstaller.DelFiles]
NicCo.dll,,,1
NicCo6.dll,,,1
NicCo36.dll,,,1
NicInVXN.dll,,,1
vxnmsg.dll,,,1

[UnInstall.DelFiles]
Prounstl.exe,,,1
vxn62x64.din,,,1

[8259x.DelReg]
HKLM,Software\Microsoft\Windows\CurrentVersion\Uninstall\PROSet
HKLM,Software\INTEL\Prounstl
HKLM,Software\INTEL\NIC

;-----------------------------------------------------------------------------
; Localizable Strings
;
[Strings]
Intel                    = "Intel"

